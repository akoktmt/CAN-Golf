#ifndef CAN_BUFFER_H_
#define CAN_BUFFER_H_

typedef struct
{
	uint8_t NodeBuffer[8][8];
	uint8_t FlagNode[8];
	uint8_t NodeIndex;
	uint8_t CheckFrameType;
	uint8_t PacketLength;
	uint8_t NumberOfFrame;
	uint8_t CRCValue;
	uint8_t FrameType;
}PowerNodeBufferHandle;

